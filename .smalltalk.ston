SmalltalkCISpec {
  #loading : [
    SCIMetacelloLoadSpec {
      #baseline : 'DiscOrDance',
      #directory : 'src',
      #platforms : [ #pharo ]
    }
  ],
  #testing : {
    #include : {
      #packages : [ 'DiscOrDance-Model-Tests-Base' ],
      #projects : [ 'BaselineOfDiscOrDance' ]
    },
    #exclude : {
      #packages : [ 'DiscOrDance-Tests', 'DiscOrDance-Tests-Base', 'DiscOrDance-Tests-Scraper' ],
      #projects : [ 'BaselineOfDiscOrDance' ]
    },
    #coverage : {
      #packages : [ 
        'DiscOrDance',
        'DiscOrDance-Base',
        'DiscOrDance-Commands',
        'DiscOrDance-Scraper-Examples',
        'DiscOrDance-Exceptions', 
        'DiscOrDance-Model*', 
        'DiscOrDance-Model-Unused', 
        'DiscOrDance-Visualization*' 
      ],
      #format : #coveralls
    }
  }
}